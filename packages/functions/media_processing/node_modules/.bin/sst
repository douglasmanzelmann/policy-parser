#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/sst@3.9.5/node_modules/sst/bin/node_modules:/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/sst@3.9.5/node_modules/sst/node_modules:/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/sst@3.9.5/node_modules:/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/sst@3.9.5/node_modules/sst/bin/node_modules:/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/sst@3.9.5/node_modules/sst/node_modules:/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/sst@3.9.5/node_modules:/Users/dmanzelmann/repos/policy-parser-v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sst/bin/sst.mjs" "$@"
else
  exec node  "$basedir/../sst/bin/sst.mjs" "$@"
fi
